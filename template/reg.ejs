<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>注册</title>
    <meta content="authenticity_token" name="csrf-param">
    <!--<meta content="{% block token %}{% endblock %}" name="csrf-token">-->
    <!-- basic scripts -->
    <script src="/static/js/bootstrap.min.js"></script>
    <script src="/static/js/ace-elements.min.js"></script>
    <script src="/static/js/ace.min.js"></script>



    <!-- basic styles -->
    <link rel="stylesheet" type="text/css" href="/static/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/static/css/bootstrap-responsive.min.css">

    <link rel="stylesheet" type="text/css" href="/static/css/font-awesome.min.css">
    <!-- page specific plugin styles -->
    <!-- ace styles -->

    <link rel="stylesheet" type="text/css" href="/static/css/ace.min.css">
    <link rel="stylesheet" type="text/css" href="/static/css/ace-responsive.min.css">
    <link rel="stylesheet" type="text/css" href="/static/css/ace-skins.min.css">
</head>
<style type="text/css">
body {
    font-family:"Helvetica Neue", Helvetica, "微软雅黑", Arial, sans-serif
}
table {
    white-space: normal;
    word-wrap: break-word;
    word-break: break-all;
}
</style>

<body>





    <div class="row">
        <!-- PAGE CONTENT BEGINS HERE -->
        <div class="span6 offset3">
            <div class="well">
                <h1 class="grey lighter smaller">
                    <span class="blue bigger-125"><i class="fa fa-sitemap"></i> 注册</span>
                </h1>
                <hr />


                <div>


                    <div class="space"></div>
                    <form action="/reg" method="POST">
	                    <h4 class="blue bigger-110">请输入:</h4>
	                    <ul class="blue bigger-110">
                            <li><i class="fa fa-hand-o-right blue">邮箱</i></li><input type="text" name="email" value="邮箱" /><% if(invalids.invalidEmail!=null){ %><%= invalids.invalidEmail %><% } %>
                            <li><i class="fa fa-hand-o-right blue">用户名</i></li><input type="text" name="userName" value="用户名" /><% if(invalids.invalidUserName!=null){ %> <%= invalids.invalidUserName %><% } %>
                            <li><i class="fa fa-hand-o-right blue">密码</i></li><input type="password" name="password" value="" /><% if(invalids.invalidPassword!=null){ %> <%= invalids.invalidPassword %><% } %>
                            <li><i class="fa fa-hand-o-right blue">密码确认</i></li><input type="password" name="checkPassword" value="" /><% if(invalids.invalidCheckPassword!=null){ %> <%= invalids.invalidCheckPassword %><% } %>
                            <li><i class="fa fa-hand-o-right blue">验证码</i></li><input type="text" name="checkCode" value="验证码" /><% if(invalids.invalidCheckCode!=null){ %> <%= invalids.invalidCheckCode %><% } %>
	                    </ul>
                </div>

                <hr />
                <div class="space"></div>

                <div class="row-fluid">
                    <div class="center">

                        <input type="submit" name="submit" class="btn btn-primary" value="注册" />
                    </div>
                </div>
                <h4 class="blue bigger-110">注意：密码登录无法使用网站全部功能，使用全部功能请使用证书登录</h4>
                </form>
            </div>
        </div>
        <!-- PAGE CONTENT ENDS HERE -->
    </div>
    <!--/row-->


</body>

</html>
